{
  "name": "ember-frost-object-details",
  "version": "7.0.0",
  "description": "The default blueprint for ember-cli addons.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "coverage": "COVERAGE=true ember test -r 'dot'",
    "lint": "lint-all-the-things",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "start": "ember serve",
    "test": "npm run lint && npm run test-addon",
    "test-addon": "EMBER_TRY_SCENARIO=${EMBER_TRY_SCENARIO:=ember-default} && ember try:one $EMBER_TRY_SCENARIO --- COVERAGE=true ember test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ciena-frost/ember-frost-object-details.git"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.8.2",
    "broccoli-asset-rev": "^2.4.5",
    "chai-jquery": "^2.0.0",
    "ember-browserify": "^1.2.0",
    "ember-cli": "2.18.2",
    "ember-cli-chai": "^0.5.0",
    "ember-cli-code-coverage": "^1.0.0-beta.3",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-frost-blueprints": "^5.0.2",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mirage": "0.2.2",
    "ember-cli-mocha": "^0.15.0",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-code-snippet": "^2.0.1",
    "ember-data": "~2.8.0",
    "ember-export-application-global": "^2.0.0",
    "ember-frost-test": "^4.0.1",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.0.0",
    "ember-sinon": "^2.1.0",
    "ember-source": "~2.18.0",
    "ember-test-utils": "^8.1.1",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^5.2.1",
    "loader.js": "^4.2.3",
    "sinon-chai": "^3.0.0"
  },
  "keywords": [
    "ember-addon",
    "frost"
  ],
  "dependencies": {
    "ember-cli-babel": "^6.6.0",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-sass": "7.1.1",
    "ember-computed-decorators": "0.3.0",
    "ember-elsewhere": "1.0.1",
    "ember-frost-core": "^7.0.0",
    "ember-hook": "^1.4.2",
    "ember-prop-types": "^7.0.2",
    "liquid-fire": "0.27.2"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
